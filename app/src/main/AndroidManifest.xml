<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ge.bog.ReceiptApp">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:name="ge.bog.ReceiptApp.presentation.MealRecipeApp"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FinalProject_nKotrikadze">
        <activity
            android:name="ge.bog.ReceiptApp.presentation.activity.CategoriesPageActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ge.bog.ReceiptApp.presentation.activity.BrowseByCategoryActivity"
            android:exported="true">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
        android:name="ge.bog.ReceiptApp.presentation.activity.ReceiptActivity"
        android:exported="true">
        <intent-filter>
            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
    </activity>


    </application>

</manifest>